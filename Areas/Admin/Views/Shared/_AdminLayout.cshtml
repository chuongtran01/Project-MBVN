<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HospitalManagementSystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/HospitalManagementSystem.styles.css" asp-append-version="true" />
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400,400italic,600,700|Raleway:300,400,500,600,700|Crete+Round:400italic" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/fonts/fontawesome-all.min.css>s
    <link rel="stylesheet" href="~/icon/themify-icons.min.css">
</head>
<body>
	<div id="app">
		<div class="sidebar app-aside" id="sidebar">
			<div class="sidebar-container">
				<nav>
					<div class="navbar-title">
						<span>Main Navigation</span>
					</div>
					<ul class="main-navigation-menu">
						<li>
							<a asp-controller="Home" asp-action="Index">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-home"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Dashboard </span>
									</div>
								</div>
							</a>
						</li>
						<li>
							<a href="javascript:void(0)">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-user"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Doctors </span><i class="icon-arrow"></i>
									</div>
								</div>
							</a>
							<ul class="sub-menu">
								<li>
									<a>
										<span class="title"> Doctor Specialization </span>
									</a>
								</li>
								<li>
									<a asp-controller="Doctor" asp-action="Create">
										<span class="title"> Add Doctor</span>
									</a>
								</li>
								<li>
									<a asp-controller="Doctor" asp-action="Index">
										<span class="title"> Manage Doctors </span>
									</a>
								</li>

							</ul>
						</li>

						<li>
							<a href="javascript:void(0)">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-user"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Users </span><i class="icon-arrow"></i>
									</div>
								</div>
							</a>
							<ul class="sub-menu">

								<li>
									<a>
										<span class="title"> Manage Users </span>
									</a>
								</li>

							</ul>
						</li>
						<li>
							<a href="javascript:void(0)">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-user"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Patients </span><i class="icon-arrow"></i>
									</div>
								</div>
							</a>
							<ul class="sub-menu">

								<li>
									<a>
										<span class="title"> Manage Patients </span>
									</a>
								</li>

							</ul>
						</li>

						<li>
							<a>
								<div class="item-content">
									<div class="item-media">
										<i class="ti-file"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Appointment History </span>
									</div>
								</div>
							</a>
						</li>



						<li>
							<a href="javascript:void(0)">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-files"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Conatctus Queries </span><i class="icon-arrow"></i>
									</div>
								</div>
							</a>
							<ul class="sub-menu">

								<li>
									<a>
										<span class="title"> Unread Query </span>
									</a>
								</li>

								<li>
									<a>
										<span class="title"> Read Query </span>
									</a>
								</li>

							</ul>
						</li>



						<li>
							<a>
								<div class="item-content">
									<div class="item-media">
										<i class="ti-list"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Doctor Session Logs </span>
									</div>
								</div>
							</a>
						</li>



						<li>
							<a asp-controller="UserLogs" asp-action="Index">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-list"></i>
									</div>
									<div class="item-inner">
										<span class="title"> User Session Logs </span>
									</div>
								</div>
							</a>
						</li>
						<li>
							<a href="javascript:void(0)">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-files"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Reports </span><i class="icon-arrow"></i>
									</div>
								</div>
							</a>
							<ul class="sub-menu">

								<li>
									<a>
										<span class="title">Between dates reports </span>
									</a>
								</li>
							</ul>
						</li>
						<li>
							<a href="javascript:void(0)">
								<div class="item-content">
									<div class="item-media">
										<i class="ti-file"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Pages </span><i class="icon-arrow"></i>
									</div>
								</div>
							</a>
							<ul class="sub-menu">

								<li>
									<a>
										<span class="title">About Us </span>
									</a>
								</li>
								<li>
									<a>
										<span class="title">Contact Us </span>
									</a>
								</li>
							</ul>
						</li>




						<li>
							<a>
								<div class="item-content">
									<div class="item-media">
										<i class="ti-search"></i>
									</div>
									<div class="item-inner">
										<span class="title"> Patient Search </span>
									</div>
								</div>
							</a>
						</li>


					</ul>

				</nav>
			</div>
		</div>
		<div class="app-content">
			<header class="navbar navbar-default navbar-static-top">
				<div class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
					<div class="container-fluid">
						<div class="nav navbar-right me-auto">
							<div style="padding-top:2% ">
								<h2>Hospital Management System</h2>
							</div>
						</div>
						<ul class="navbar-nav">
							<li class="nav-item">
								<a>Log out</a>
							</li>
							<li class="nav-item">
								<a>Change Password</a>
							</li>
						</ul>
					</div>
				</div>
			</header>

			<div class="main-content">
				<div class="wrap-content container" id="container">
					@RenderBody()
				</div>
			</div>
		</div>
	</div>

    <footer class="border-top footer text-muted">
        <div class="container">
            copy; 2023 - HospitalManagementSystem - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-cookie/jquery.cookie.js"></script>
    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

